copy /Y "$(ProjectDir)\Distribution\GameData\$(Targetname)\configs\*.*" "%25KSP_DIR%25\GameData\$(Targetname)\Configs\"
copy /Y "$(TargetPath)" "%25KSP_DIR%25\GameData\$(Targetname)\Plugins\"
copy /Y "$(TargetDir)$(Targetname).pdb" "%25KSP_DIR%25\GameData\$(Targetname)\Plugins\"
copy /Y "$(TargetDir)CLSInterfaces.dll" "%25KSP_DIR%25\GameData\$(Targetname)\Plugins\"

echo distributing $(Targetname) files...
del /Q "%25DIST_DIR%25\Distribution\GameData\$(Targetname)\Configs\*.*"
del /Q "%25DIST_DIR%25\Distribution\GameData\$(Targetname)\assets\*.*"
del /Q "%25DIST_DIR%25\Distribution\GameData\$(Targetname)\Localization\*.*"

copy /Y "$(TargetPath)" "%25DIST_DIR%25\Distribution\GameData\$(Targetname)\Plugins\"
copy /Y "$(TargetDir)\CLSInterfaces.dll" "%25DIST_DIR%25\Distribution\GameData\$(Targetname)\Plugins\"
copy /Y "$(ProjectDir)\Distribution\GameData\$(Targetname)\Localization\*.*" "%25DIST_DIR%25\Distribution\GameData\$(Targetname)\Localization\"
copy /Y "$(ProjectDir)\Distribution\GameData\$(Targetname)\assets\*.*" "%25DIST_DIR%25\Distribution\GameData\$(Targetname)\assets\"
